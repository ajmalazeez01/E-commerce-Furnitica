                        </div>
                        <div class="col-md-2 text-center d-flex justify-content-center align-items-center">
                          <p class="text-muted mb-0"><%= data.name %> </p>
                        </div>
                        <div class="col-md-2 text-center d-flex justify-content-center align-items-center">
                          <p class="text-muted mb-0 small">Price -<%= data.price %> </p>
                        </div>
                        
                        <div class="col-md-2 text-center d-flex justify-content-center align-items-center">
                          <p class="text-muted mb-0 small">Qty -<%= data. productQty %> </p>
                        </div>
                        <div class="col-md-3 text-center d-flex justify-content-center align-items-center">
                          <p class="text-muted mb-0 small"><%= data.total %> </p>
                        </div>
                      </div>
                     
                    </div>
                  </div>
                  <% }) %> 
                     <div class="row d-flex align-items-center">
                        <div class="col-md-2">
                          <p class="text-muted mb-0 small">Track Order</p>
                        </div>
                        <div class="col-md-10">
                            <% if(productData[0].orderStatus =='processing') {%> 
                                <div class="progress" style="height: 6px; border-radius: 16px;">
                                    <div class="progress-bar" role="progressbar"

                                     style="width: 12%; border-radius: 16px; background-color: #51e63a;" aria-valuenow="12"
                                      aria-valuemin="0" aria-valuemax="100">
                                    </div>
                                  </div>
                                  <div class="d-flex justify-content-around mb-1">
                                    <p class="text-muted mt-1 mb-0 small ms-xl-5">Processing</p>
                                    <p class="text-muted mt-1 mb-0 small ms-xl-5">Packed</p>
                                    <p class="text-muted mt-1 mb-0 small ms-xl-5">Shipped</p>
                                    <p class="text-muted mt-1 mb-0 small ms-xl-5">Out for Delivered</p>
                                  </div>
                                <% }else if(productData[0].orderStatus=='packed') {%> 
                                    <div class="progress" style="height: 6px; border-radius: 16px;">
                                        <div class="progress-bar" role="progressbar"
                                          style="width: 37%; border-radius: 16px; background-color: #51e63a;" aria-valuenow="37"
                                          aria-valuemin="0" aria-valuemax="100">
                                        </div>
                                      </div>
                                      <div class="d-flex justify-content-around mb-1">
                                        <p class="text-muted mt-1 mb-0 small ms-xl-5">Processing</p>
                                        <p class="text-muted mt-1 mb-0 small ms-xl-5">Packed</p>
                                        <p class="text-muted mt-1 mb-0 small ms-xl-5">Shipped</p>
                                        <p class="text-muted mt-1 mb-0 small ms-xl-5">Out for Delivered</p>
                                      </div>
                                    <% }else if(productData[0].orderStatus=='shipped') { %> 
                                        <div class="progress" style="height: 6px; border-radius: 16px;">
                                            <div class="progress-bar" role="progressbar"
                                              style="width: 59%; border-radius: 16px; background-color: #51e63a;" aria-valuenow="59"
                                              aria-valuemin="0" aria-valuemax="100">
                                            </div>
                                          </div>
                                          <div class="d-flex justify-content-around mb-1">
                                            <p class="text-muted mt-1 mb-0 small ms-xl-5">Processing</p>
                                            <p class="text-muted mt-1 mb-0 small ms-xl-5">Packed</p>
                                            <p class="text-muted mt-1 mb-0 small ms-xl-5">Shipped</p>
                                            <p class="text-muted mt-1 mb-0 small ms-xl-5">Out for Delivered</p>
                                          </div>
                                          <% }else if(productData[0].orderStatus=='cancel') { %> 
                                            <div class="progress" style="height: 6px; border-radius: 16px;">
                                                <div class="progress-bar" role="progressbar"
                                                  style="width: 100%; border-radius: 16px; background-color: #c82828;" aria-valuenow="100"
                                                  aria-valuemin="0" aria-valuemax="100">
                                                </div>
                                              </div>
                                              <div class="d-flex justify-content-around mb-1">
                                                
                                            <p class="text-muted mt-1 mb-0 small ms-xl-5">Cancelled</p>
                                              </div>
                                                <% }else{ %>
                                                    <div class="progress" style="height: 6px; border-radius: 16px;">
                                                        <div class="progress-bar" role="progressbar"
                                                            style="width: 100%; border-radius: 16px; background-color: #51e63a;" aria-valuenow="37"
                                                            aria-valuemin="0" aria-valuemax="100">
                                                        </div>
                                                        </div>
                                                        <div class="d-flex justify-content-around mb-1">
                                                        <p class="text-muted mt-1 mb-0 small ms-xl-5">Processing</p>
                                                        <p class="text-muted mt-1 mb-0 small ms-xl-5">Packed</p>
                                                        <p class="text-muted mt-1 mb-0 small ms-xl-5">Shipped</p>
                                                        <p class="text-muted mt-1 mb-0 small ms-xl-5">Out for Delivered</p>
                                                        </div>
                                                    <% } %>  
                     
                        </div>
                      </div>  
                  <div class="d-flex justify-content-between pt-2">
                    <p class="font-weight-medium mb-0">Order Address</p>
                    <p class="font-weight-medium mb-0"><span class="font-weight-bold me-4">Total₹ -</span> <%= productData[0].total %> </p>
                  </div>
      
                  <div class="d-flex justify-content-between pt-1">
                    <p class="text-muted mb-0">
                        <%= productData[0].address.addressline1 %> <br>
                        <%= productData[0].address.name %> <br>
                        <%= productData[0].address.address %> ,
                        <%= productData[0].address.district %> ,
                        <%= productData[0].address.state %> ,
                        <%= productData[0].address.country %>  <br>
                        <%= productData[0].address.pin%> <br>
                        <%= productData[0].address.mobile %> 
                     </p>
                     <% if(productData.status !='Cancel') {%> 
                        <a style="height: 40px;display: block;" href="/ " class="btn btn-dark mt-4 px-3">Cancel</a>
                     <% } %> 
                    
                  </div>
                </div>
               
              </div>
            </div>
          </div>
