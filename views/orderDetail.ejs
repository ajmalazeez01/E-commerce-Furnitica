              <div class="card" style="border-radius: 10px;">
                
                <div class="card-body p-4">
                 
                  <% productData.forEach((data)=>{ %> 
                  <div class="card shadow-0 border mb-4">
                    <div class="card-body">
                      <div class="row">
                        <div class="col-md-3">
                          <img src="/public/proimg/<%= data.image %> "
